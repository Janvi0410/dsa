//How can you remove duplicates in an array?


#include<stdio.h>
int main()
{
     int n, a[100], b[100], calc = 0, i, j,count;
     printf("Enter no. of elements in array.n");
     scanf("%d", &amp;n);
     printf("Enter %d integersn", n);
     for (i = 0; i &lt; n; i++)
           scanf("%d", &amp;a[i]);
           for (i = 0; i&lt;n; i++) 
           {
                 for (j = 0; j&lt;calc; j++) 
                 {
                        if(a[i] == b[j])
                        break;   
                  }
           if (j== calc)
           {
                  b[count] = a[i];
                  calc++;  
            }
      }
      printf("Array obtained after removing duplicate elementsn");
      for (i = 0; i&lt;calc; i++)
      {  
             printf("%dn", b[i]);
      }
      return 0;
}v
